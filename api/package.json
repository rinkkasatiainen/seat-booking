{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.2",
    "ws": "^8.13.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start": "yarn clean && tsc && node dist/src/app.js",
    "start:dev": "concurrently --kill-others \"tsc -w\" \"nodemon\"",
    "lint": "eslint -- src test",
    "lint:fix": "eslint --fix -- src test",
    "test": "NODE_ENV=test ts-mocha -p ./tsconfig.test.json test/**/*.test.ts",
    "test:unit": "NODE_ENV=test ts-mocha -p ./tsconfig.test.json test/unit/**/*.test.ts",
    "test:component": "NODE_ENV=test ts-mocha -p ./tsconfig.test.json test/component/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.10",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chai": "^4.3.7",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
